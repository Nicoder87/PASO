@model Escrutinio.Models.VotosModel

@{
    var tipo = Model.TIPO;
}

<h4>VOTOS</h4>

@* //////////// INTENDENTE /////////////// *@

<div class="row">
    <div class="cabe col-xs-12" style="font-weight:700">
        INTENDENTE
    </div>
</div>

@foreach (var item in Model.LISTAS_COMPLETAS)
{
    if (item.INTENDENTE != null)
    {
        <div class="row">
            <div class="pad2 col-xs-6">
                @item.Code @item.LISTA_CORTA
            </div>
            <div class="pad2 col-xs-6">
                @item.INTENDENTE
            </div>
        </div>
    }
}

